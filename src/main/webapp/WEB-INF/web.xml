<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>ServletPlannetPro</servlet-name>
        <servlet-class>com.mycompany.Controller.ServletPlannetPro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioPlannetPro</servlet-name>
        <servlet-class>com.mycompany.Controller.UsuarioPlannetPro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createEventoServ</servlet-name>
        <servlet-class>com.mycompany.Controller.createEventoServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletPlannetPro</servlet-name>
        <url-pattern>/ServletPlannetPro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioPlannetPro</servlet-name>
        <url-pattern>/UsuarioPlannetPro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createEventoServ</servlet-name>
        <url-pattern>/createEventoServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
